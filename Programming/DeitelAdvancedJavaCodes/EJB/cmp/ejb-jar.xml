<?xml version="1.0"?>

<!DOCTYPE ejb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 2.0//EN"
"http://java.sun.com/dtd/ejb-jar_2_0.dtd">

<ejb-jar>
  <enterprise-beans>
    <entity>
      <ejb-name>CMPProduct</ejb-name>
      <home>cmp.CMPProductHome</home>
      <remote>cmp.CMPProduct</remote>
      <ejb-class>cmp.CMPProductBean</ejb-class>
      <persistence-type>Container</persistence-type>
      <prim-key-class>java.lang.String</prim-key-class>
      <reentrant>False</reentrant>
      <cmp-field><field-name>productId</field-name></cmp-field>
      <cmp-field><field-name>productName</field-name></cmp-field>
      <cmp-field><field-name>description</field-name></cmp-field>
      <cmp-field><field-name>price</field-name></cmp-field>
      <primkey-field>productId</primkey-field>
    </entity>
  </enterprise-beans>
</ejb-jar>
