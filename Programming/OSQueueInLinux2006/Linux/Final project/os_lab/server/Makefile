	@mkdir -p .tmp/server && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/server/ && $(COPY_FILE) --parents serverform.ui.h .tmp/server/ && ( cd `dirname .tmp/server` && $(TAR) server.tar server && $(GZIP) server.tar ) && $(MOVE) `dirname .tmp/server`/server.tar.gz . && $(DEL_FILE) -r .tmp/server

