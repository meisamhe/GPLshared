<?xml version="1.0" encoding="UTF-8"?>

<portlet-app version="1.0" xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_1_0.xsd">

    <portlet>
        <description xml:lang="EN">Calendar Portlet</description>
        <portlet-name>MyPortlet</portlet-name>
        <display-name xml:lang="EN">MyPortlet</display-name>
        <portlet-class>MyPortlet</portlet-class>
        <expiration-cache>-1</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>MyPortlet</title>
            <short-title>Calendar</short-title>
            <keywords>Calendar</keywords>
        </portlet-info>
    </portlet>
    <portlet>
        <description xml:lang="EN">Search Product</description>
        <portlet-name>SearchManager</portlet-name>
        <display-name xml:lang="EN">SearchManager</display-name>
        <portlet-class>SearchManager</portlet-class>
        <expiration-cache>-1</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>SearchManager</title>
        </portlet-info>
    </portlet>
    <portlet>
        <description xml:lang="EN">Cart</description>
        <portlet-name>CartManager</portlet-name>
        <display-name xml:lang="EN">CartManager</display-name>
        <portlet-class>CartManager</portlet-class>
        <expiration-cache>-1</expiration-cache>
        <supports>
            <mime-type>text/html</mime-type>
            <portlet-mode>edit</portlet-mode>
        </supports>
        <supported-locale>en</supported-locale>
        <portlet-info>
            <title>CartManager</title>
        </portlet-info>
    </portlet>

</portlet-app>
